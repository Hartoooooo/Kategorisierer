# Finnhub API Konfiguration
FINNHUB_BASE_URL=https://finnhub.io/api/v1
FINNHUB_SECRET=dein_finnhub_api_key_hier

# Optional: Concurrency Limit für parallele API-Requests (Standard: 17)
# Erhöhen Sie diesen Wert für schnellere Verarbeitung, aber beachten Sie Rate Limits
# FINNHUB_CONCURRENCY_LIMIT=17

# Supabase Konfiguration
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-anon-key

# Upload Limits
APP_MAX_UPLOAD_MB=10
